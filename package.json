{
    "name": "clonifire",
    "version": "0.1.1",
    "description": "this package is designed to clone git repositories following the path used to organize repos on Gitlab",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "patch": "versionifier patch",
        "minor": "versionifier minor",
        "major": "versionifier major"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/hyphaene/clonifire.git"
    },
    "keywords": [
        "cli",
        "folder",
        "gitlab",
        "git",
        "clone",
        "path"
    ],
    "author": "Maximilien Garenne <maximilien.garenne@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/hyphaene/clonifire/issues"
    },
    "homepage": "https://github.com/hyphaene/clonifire#readme",
    "devDependencies": {
        "eslint": "^5.0.1",
        "versionifier": "^1.3.4"
    },
    "versionifier": {
        "commitIfOnlyPackageJsonInStage": true,
        "commitIfMultipleFilesInStage": false,
        "push": true,
        "commitMessage": "Updated package.json to version @@",
        "remoteRepo": "origin",
        "publish": true
    },
    "dependencies": {
        "execa": "^0.10.0",
        "os": "^0.1.1"
    }
}